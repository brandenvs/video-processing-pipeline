# Copy this file to .env and fill with real values. DO NOT commit .env
# Database credentials
POSTGRES_USER=postgres
POSTGRES_PASSWORD=replace_with_strong_password
POSTGRES_DB=postgres
POSTGRES_HOST=db

# Vectorizer worker DB URL (compose uses this for the worker)
# Example: postgres://postgres:replace_with_strong_password@db:5432/postgres
PGAI_VECTORIZER_WORKER_DB_URL=postgres://${POSTGRES_USER}:${POSTGRES_PASSWORD}@${POSTGRES_HOST}:5432/${POSTGRES_DB}

# Optional: override ollama host
OLLAMA_HOST=http://ollama:11434
